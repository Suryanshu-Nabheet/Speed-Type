{
  "name": "speed-type",
  "version": "1.0.0",
  "description": "A free, customizable typing test and practice tool with multiplayer support",
  "type": "module",
  "main": "dist/cli/index.js",
  "bin": {
    "speed-type": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx cli/index.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "start": "node dist/cli/index.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint && npm run build"
  },
  "keywords": [
    "typing",
    "test",
    "cli",
    "terminal",
    "wpm",
    "practice",
    "multiplayer",
    "race"
  ],
  "author": "Speed Type Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@fastify/cors": "^8.4.2",
    "@fastify/websocket": "^8.3.1",
    "@types/ink": "^0.5.2",
    "better-sqlite3": "^9.2.2",
    "blessed": "^0.1.81",
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "commander": "^11.1.0",
    "conf": "^12.0.0",
    "fastify": "^4.25.2",
    "figlet": "^1.7.0",
    "ink": "^4.4.1",
    "ink-testing-library": "^3.0.0",
    "nanoid": "^5.0.4",
    "ora": "^8.0.1",
    "react": "^18.3.1",
    "ws": "^8.16.0",
    "yoga-wasm-web": "^0.3.3"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/blessed": "^0.1.25",
    "@types/figlet": "^1.5.8",
    "@types/jest": "^29.5.10",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.15",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  },
  "files": [
    "dist/",
    "assets/",
    "README.md",
    "LICENSE"
  ]
}
